SKYHUBButton.Position = UDim2.new(0.063415885, 0, 0.371442765, 0)
SKYHUBButton.Size = UDim2.new(0, 180, 0, 45)
SKYHUBButton.ZIndex = 2
SKYHUBButton.Font = Enum.Font.GothamSemibold
SKYHUBButton.Text = ""
SKYHUBButton.TextColor3 = Color3.fromRGB(255, 255, 255)
SKYHUBButton.TextScaled = true
SKYHUBButton.TextSize = 14.000
SKYHUBButton.TextWrapped = true
SKYHUBButton.MouseButton1Click:Connect(function()
	loadstring(game:HttpGet(('https://raw.githubusercontent.com/NoobHackel/Roblox-Stands-Awakening-Hub/main/SKY-HUB'),true))()
end)

